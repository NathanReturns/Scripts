local function Connect()
if syn.websocket then ws = syn.websocket.connect("wss://RoPhone.nathanreturns.repl.co") end
if WebSocket then ws = WebSocket.connect("wss://RoPhone.nathanreturns.repl.co") end
if WebsocketClient then ws = WebsocketClient.new("wss://RoPhone.nathanreturns.repl.co") end
end

Connect()
ws.OnClose:Connect(function()
    Connect()
end)

pcall(function() getgenv().RoPhoneLoaded = true end)
